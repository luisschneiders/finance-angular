{
  "name": "finance-angular",
  "version": "1.0.0",
  "description": "App to control your finances",
  "scripts": {
    "build": "gulp build",
    "build:production": "gulp build --production",
    "postinstall": "npm run build:production",
    "start": "node server.js",
    "test": "npm run test:server && npm run test:client",
    "test:client": "karma start app/karma.conf.js --single-run",
    "test:server": "mocha --recursive",
    "test-coverage": "nyc --reporter=html --reporter=text mocha",
    "watch": "gulp",
    "pm2-restart": "pm2 restart server.js",
    "pm2-stop": "pm2 stop server.js"
  },
