    "dotenv": "^2.0.0",
    "express": "^4.17.3",
    "express-validator": "^2.21.0",
    "graceful-fs": "^4.1.11",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "latest",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "latest",
    "gulp-csso": "^1.1.0",
    "gulp-if": "^2.0.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-ng-config": "^1.2.1",
